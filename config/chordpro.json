	// Papersize, 'a4' or [ 595, 842 ] etc.
	"papersize" : "a4",

	// Space between columns, in pt.
	"columnspace"  :  20,

	// Page margins.
	// Note that top/bottom exclude the head/footspace.
	"margintop"    :  80,
	"marginbottom" :  40,
	"marginleft"   :  40,
	"marginright"  :  40,
	"headspace"    :  60,
	"footspace"    :  20,

	// Special: head on first page only, add the headspace to
