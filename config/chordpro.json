	    // By default, a page has:
	    "default" : {
	        // No title/subtitle.
	    	"title"     : null,
	    	"subtitle"  : null,
		// Footer is title -- page number.
	    	"footer"    : [ "%{title}", "", "%{page}" ],
	    },
	    // The first page of a song has:
	    "title" : {
	        // Title and subtitle.
	    	"title"     : [ "", "%{title}", "" ],
	    	"subtitle"  : [ "", "%{subtitle}", "" ],
        "artist"    : [ "", "%{artist}", "" ],
		// Footer with page number.
	    	"footer"    : [ "", "", "%{page}" ],
	    },
	    // The very first output page is slightly different:
	    "first" : {
